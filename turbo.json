{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "typecheck": {
      "dependsOn": ["build"]
    },
    "lint": {
      "dependsOn": []
    },
    "bench": {
      "dependsOn": ["build"]
    }
  }
}
